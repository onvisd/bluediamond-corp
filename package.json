{
  "name": "bluediamond-corp",
  "version": "0.1.0",
  "description": "Blue Diamond Corporate Website",
  "repository": {
    "type": "git",
    "url": "https://github.com/planetary/bluediamond-corp.git"
  },
  "homepage": "https://github.com/planetary/bluediamond-corp",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm-run-all development:client:render-service:build:prepare development",
    "development": "npm-run-all --parallel development:client:build development:client:render-service:build development:services",
    "development:client:render-service:build:prepare": "universal-webpack --settings ./client/webpack/universal-webpack-settings.json --config ./client/webpack/webpack.config.js prepare",
    "development:client:build": "babel-node ./client/webpack/webpack-dev-server.js",
    "development:client:render-service:build": "webpack --config ./client/webpack/webpack.config.server.development.babel.js --watch --colors --display-error-details",
    "development:api": "nodemon ./api/index.js --watch ./api",
    "development:client:render-service": "nodemon ./client/render-service/index.js --watch ./client/render-service --watch ./client/build/server",
    "development:server": "nodemon ./server/index.js --watch ./server",
    "development:services": "npm-run-all --parallel development:api development:client:render-service development:server",
    "production": "npm-run-all production:client:build production:client:render-service:build production:services",
    "production:client:build": "webpack --colors --display-error-details --config ./client/webpack/webpack.config.client.production.babel.js",
    "production:client:render-service:build": "webpack --colors --display-error-details --config ./client/webpack/webpack.config.server.production.babel.js",
    "production:api": "better-npm-run production:api",
    "production:client:render-service": "better-npm-run production:client:render-service",
    "production:server": "better-npm-run production:server",
    "production:services": "npm-run-all --parallel production:api production:client:render-service production:server"
  },
  "betterScripts": {
    "production:client:render-service": {
      "command": "node ./client/render-service/index.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "production:api": {
      "command": "node ./api/index.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "production:server": {
      "command": "node ./server/index.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "author": "Planetary Corporation",
  "license": "Proprietary",
  "dependencies": {
    "autoprefixer": "^6.6.1",
    "axios": "^0.15.3",
    "babel-eslint": "^7.2.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-plugin-transform-react-display-name": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "better-npm-run": "0.0.13",
    "bluebird": "^3.4.7",
    "body-parser": "^1.17.1",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.14",
    "css-loader": "^0.26.1",
    "dotenv": "^4.0.0",
    "eslint": "^3.18.0",
    "eslint-config-planetary": "^0.1.1",
    "eslint-plugin-react": "^6.10.3",
    "express": "^4.15.2",
    "express-http-proxy": "^0.11.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.9.0",
    "lodash": "^4.17.4",
    "marked": "^0.3.6",
    "morgan": "^1.8.1",
    "node-sass": "^4.1.1",
    "npm-run-all": "^4.0.0",
    "postcss-cssnext": "^2.10.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.2.1",
    "postcss-mixins": "^5.4.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-isomorphic-render": "^10.1.0",
    "react-redux": "^5.0.2",
    "react-responsive-ui": "^0.4.7",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "sass-loader": "^6.0.0",
    "simpler-redux-form": "^0.3.9",
    "style-loader": "^0.13.1",
    "stylelint": "^7.9.0",
    "stylelint-config-planetary": "^1.0.1",
    "universal-webpack": "^0.3.3",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0-rc.6"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "nodemon": "^1.11.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.2.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.15.0"
  }
}
